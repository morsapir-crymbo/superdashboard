{
  "name": "vercel-nest-next-envs",
  "private": true,
  "packageManager": "pnpm@10.17.1",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:api": "pnpm --filter api dev",
    "dev:web": "pnpm --filter web dev",
    "dev": "concurrently -k \"pnpm dev:api\" \"pnpm dev:web\"",
    "build": "pnpm -r build",
    "prisma:generate": "pnpm --filter db prisma generate",
    "db:migrate": "pnpm --filter api prisma migrate deploy"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "turbo": "^2.5.8"
  }
}